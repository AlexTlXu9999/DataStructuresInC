#include<stdio.h>
#include<malloc.h>
#define OK 1
#define ERROR 0
#define MAXSIZE 100
typedef struct {
	ElemType elem[MAXSIZE];
	int last;
}SeqList;
int Locate(SeqList L, ElemType e) {
	int i = 0;
	while((i<=L.last)&&(L.elem[i]!=e))
	{
		i++;
	}
	if (i <= L.last)
	{
		return (i + 1);
	}
	else
		return(-1);
}
int InsList(SeqList* L, int i, ElemType e) {
	int k;
	if ((i < k) || (i > L->last + 2)) {
		printf("插入位置i值不合法");
		return(ERROR);
	}
	if (L->last = MAXSIZE - 1) {
		printf("表已满，无法插入");
		return(ERROR);
	}
	for(k = L->last; k >= i - 1; k--) {
		L->elem[k + 1] = L->elem[k];
	}
	L->elem[i - 1] = e;
	L->last++;
}
int DelList(SeqList* L, int i, ElemType* e) {
	int k;
	if ((i < 1) || (i > L->last + 1)) {
		printf("删除位置不合法");
		return(ERROR);
	}
	*e = L->elem[i - 1];
	for (k = i; i <= L->last; k++) {
		L->elem[k - 1] = L->elem[k];
	}
	L->last--;
	return(OK);
}
void main() {

}
